
<h1>Welcome <%= @caregiver.name %><br></h1>

<%= render partial: "caregiver", locals: { caregiver: @caregiver } %>

<h2>All Babies in your Care</h2>
<% @caregiver.babies.each do |baby| %>
  <%= link_to baby.name, baby_path(baby) %> - <%= link_to 'Add a log for this baby', new_baby_dailylog_path(baby) %><br>
<% end %>

<%= link_to 'Edit', edit_caregiver_path %><br>
