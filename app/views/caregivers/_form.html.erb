<%= form_for @caregiver, html: {multipart: true} do |f| %>
  Name <br>
  <%= f.text_field :name %><br>
  Email <br>
  <%= f.text_field :email %><br>

  Upload your picture(paste a link)<br>
  <%= f.file_field :avatar%><br>

  Primary Contact number<br>
  <%= f.text_field :phone_number%><br>

  How many years of childcare experience do you have <br>
  <%= f.text_field :experience %><br>

  Age(in years)<br>
  <%= f.text_field :age %><br>

  Home Address<br>
  <%= f.text_field :address %><br>

  Password
  <%= f.password_field :password %><br>

  <%= f.submit %>
<% end %>
